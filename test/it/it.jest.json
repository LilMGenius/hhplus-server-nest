{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "modulePathIgnorePatterns": ["<rootDir>/dist/"],
  "rootDir": "../..",
  "testEnvironment": "node",
  "testRegex": ".it.spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "@swc/jest"
  },
  "globalSetup": "./test/it/setup.ts",
  "globalTeardown": "./test/it/down.ts"
}
